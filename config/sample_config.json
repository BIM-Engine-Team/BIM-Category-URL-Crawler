      "about",
      "legal",
      "privacy",
      "terms",
      "login",
      "register",
      "account",
      "cart",
      "checkout",
      "support"
    ],
    "prioritize_patterns": [
      "products",
      "catalog",
      "categories",
      "specifications",
      "materials",
      "gallery",
      "portfolio"
    ]
