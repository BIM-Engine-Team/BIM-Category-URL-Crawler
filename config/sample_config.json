      "roofing",
      "flooring",
      "siding",
      "tiles",
      "lumber",
      "hardware",
      "fixtures",
      "materials",
      "construction",
      "building",
      "architectural",
      "commercial",
      "residential"
    ],
    "skip_patterns": [
